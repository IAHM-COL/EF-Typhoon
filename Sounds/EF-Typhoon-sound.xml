	 <name>Reheat internal</name>
	 <mode>looped</mode>
	 <path>Aircraft/EF-Typhoon/Sounds/Samples/reheat.wav</path>
	 <condition>
	 <and>
	 <property>/sim/current-view/internal</property>
	 </and>
	 <and>
	 <property>engines/engine/afterburner</property>
	 </and>
