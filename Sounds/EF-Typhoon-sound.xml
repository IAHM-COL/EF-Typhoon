    <!-- no binding yet? 
	 <stall>
	 <name>stall</name>
	 <path>Aircraft/EF-Typhoon/Sounds/Samples/stall.wav</path>
	 <property>/sim/alarms/stall-warning</property>
	 <volume>
	 <property>/velocities/airspeed-kt</property>
	 <min>30.0</min>
	 </volume>
	 </stall>
    -->

    <!-- Stall testing from A340 -->
    <stall>
      <name>stall</name>
      <mode>looped</mode>
      <path>Aircraft/EF-Typhoon/Sounds/Samples/stall.wav</path>
      <condition>
        <less-than>
          <property>velocities/airspeed-kt</property>
          <value>90</value>
        </less-than>
        <not>
          <property>/gear/gear[0]/wow</property>
          <property>/gear/gear[1]/wow</property>
          <property>/gear/gear[2]/wow</property>
          <property>/gear/gear[0]/position-norm</property>
        </not>
      </condition>
      <volume>
        <max>0.5</max>
      </volume>
      <position>
        <x>-22</x>
        <y>0</y>
        <z>0</z>
      </position>
      <reference-dist>1.0</reference-dist>
      <max-dist>2.0</max-dist>
    </stall>

    <!-- switches -->
    <click>
      <name>hook click down</name>
      <path>Aircraft/EF-Typhoon/Sounds/Samples/click.wav</path>
      <condition>
	<property>/controls/gear/tailhook</property>
      </condition>
      <volume>
	<factor>1.0</factor>
	<max>1.0</max>
      </volume>
    </click>
    <click>
      <name>hook click up</name>
      <path>Aircraft/EF-Typhoon/Sounds/Samples/click.wav</path>
      <condition>
	<not>
	  <property>/controls/gear/tailhook</property>
	</not>
      </condition>
      <volume>
	<factor>1.0</factor>
	<max>1.0</max>
      </volume>
    </click>
    <click>
      <name>tailwheel-lock click down</name>
      <path>Aircraft/EF-Typhoon/Sounds/Samples/click.wav</path>
